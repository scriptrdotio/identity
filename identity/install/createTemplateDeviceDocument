//run this file once to create the CSV template for users to download from the Import Devices panel
var fileContent = "name,id,description,status,Attribute1:string,Attribute2:text,Attribute3:numeric\r\ndev1,Device1,This is the device description,active,stringAttr,,123\r\ndev2,Device2,,stringAttr,stringAttr,textAttr,\r\ndev3,Device3,,active,,,555";

var fileName = "devicesTemplate.csv";
var result = apsdb.createFile(fileContent,fileName);
var docKey = "devicesTemplate";
var reportData = {
    "key": docKey,
    "attachments": result.file
};
var defaultStore = require('document').getInstance('DefaultStore');
return defaultStore.save(reportData);