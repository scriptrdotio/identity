//run this file once to create the CSV template for users to download from the Import Devices panel
var fileContent = "id,name,email,status,Attribute1:string,Attribute2:text,Attribute3:numeric\r\nuser1,User1,user1@gmail.com,active,stringAttr,,123\r\nuser2,User2,,stringAttr,stringAttr,textAttr,\r\nuser3,User3,,suspended,,,555";

var fileName = "usersTemplate.csv";
var result = apsdb.createFile(fileContent,fileName);
var docKey = "usersTemplate";
var reportData = {
    "key": docKey,
    "attachments": result.file
};
var defaultStore = require('document').getInstance('DefaultStore');
return defaultStore.save(reportData);